<addon id="plugin.video.hulu"
       name="Hulu"
       version="2.5.6"
       provider-name="BlueCop">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    <import addon="script.module.beautifulsoup" version="3.0.8"/>
    <import addon="script.module.cryptopy" version="1.2.5"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
             library="default.py">
	<provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <minversion>29000</minversion>
    <summary>Plugin for Hulu videos</summary>
    <description>View videos on Hulu</description>
    <supportedcontent>
      <content>movies</content>
      <content>tvshows</content>
    </supportedcontent>
  </extension>
</addon>

